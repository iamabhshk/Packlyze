{
  "name": "packlyze",
  "version": "4.0.1",
  "description": "Advanced bundle analyzer with insights, recommendations, and historical tracking",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "packlyze": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --fix",
    "format": "prettier --write src",
    "prepublishOnly": "npm run build && npm run test && npm run lint"
  },
  "keywords": [
    "bundle",
    "analyzer",
    "optimization",
    "performance",
    "webpack",
    "rollup",
    "esbuild",
    "tree-shaking",
    "bundle-size",
    "code-splitting"
  ],
  "author": "Abhishek Srinivasan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamabhshk/packlyze.git"
  },
  "bugs": {
    "url": "https://github.com/iamabhshk/packlyze/issues"
  },
  "homepage": "https://github.com/iamabhshk/packlyze#readme",
  "dependencies": {
    "commander": "^12.0.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.0",
    "table": "^6.8.0"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "@types/node": "^20.10.0",
    "vitest": "^1.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0"
  }
}